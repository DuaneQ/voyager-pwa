{
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "javascript": true,
    "typescript": true
  },
  "editor.inlineSuggest.enabled": false,
  "github.copilot.advanced": {
    "listCount": 5,
    "inlineSuggestCount": 3
  },
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.suggest.autoImports": false,
  "javascript.suggest.autoImports": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.surveys.enabled": false,
  "workbench.enableExperiments": false,
  "extensions.autoCheckUpdates": false,
  "typescript.preferences.includeCompletionsWithSnippetText": false,
  "typescript.suggest.completeFunctionCalls": false,
  "javascript.suggest.completeFunctionCalls": false,
  "editor.quickSuggestions": {
    "strings": false,
    "comments": false,
    "other": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.git/**": true,
    "**/coverage/**": true,
    "**/cypress/videos/**": true,
    "**/cypress/screenshots/**": true,
    "**/cypress/downloads/**": true,
    "**/.next/**": true,
    "**/.cache/**": true,
    "**/tmp/**": true,
    "**/temp/**": true,
    "**/*.log": true,
    "**/.DS_Store": true,
    "**/.firebase/**": true,
    "**/functions/lib/**": true,
    "**/scripts/**": true
  },
  "files.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.git": true,
    "**/coverage": true,
    "**/cypress/videos": true,
    "**/cypress/screenshots": true,
    "**/cypress/downloads": true,
    "**/.next": true,
    "**/.cache": true,
    "**/tmp": true,
    "**/temp": true,
    "**/*.log": true,
    "**/.DS_Store": true,
    "**/pglite-debug.log": true,
    "**/.firebase": true,
    "**/functions/lib": true,
    "**/scripts": true,
    "**/import-itineraries.js": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true,
    "**/coverage": true,
    "**/cypress/videos": true,
    "**/cypress/screenshots": true,
    "**/cypress/downloads": true,
    "**/.next": true,
    "**/.cache": true,
    "**/tmp": true,
    "**/temp": true,
    "**/*.log": true,
    "**/.DS_Store": true,
    "**/pglite-debug.log": true,
    "**/.firebase": true,
    "**/functions/lib": true,
    "**/scripts": true,
    "**/import-itineraries.js": true
  }
}